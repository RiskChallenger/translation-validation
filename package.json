{
  "name": "translation-validation",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "release:first": "npm run release -- --first-release",
    "deploy": "ng deploy",
    "copy": "cp -r README.md dist/ngx-translation-validation",
    "build:lib": "ng build ngx-translation-validation --configuration=production && npm run copy",
    "test:lib": "ng test ngx-translation-validation",
    "release": "cd projects/ngx-translation-validation && standard-version --infile ../../CHANGELOG.md",
    "test:lib:headless": "cross-env CI=true npm run test:lib",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.8",
    "@angular/common": "18.2.8",
    "@angular/compiler": "18.2.8",
    "@angular/core": "18.2.8",
    "@angular/forms": "18.2.8",
    "@angular/platform-browser": "18.2.8",
    "@angular/platform-browser-dynamic": "18.2.8",
    "@angular/router": "18.2.8",
    "@ngneat/lib": "5.0.0",
    "@ngneat/transloco": "4.1.1",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.8",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "18.2.8",
    "@angular/compiler-cli": "18.2.8",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-angular": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@ngneat/spectator": "10.0.1",
    "@ngneat/transloco-keys-manager": "3.4.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "4.6.4",
    "@types/node": "16.18.113",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "all-contributors-cli": "6.26.1",
    "angular-cli-ghpages": "1.0.7",
    "cross-env": "7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "git-cz": "4.9.0",
    "husky": "9.1.6",
    "jasmine-core": "5.3.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.10",
    "ng-packagr": "18.2.1",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "protractor": "7.0.0",
    "semantic-release": "24.1.2",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RiskChallenger/translation-validation.git"
  }
}
