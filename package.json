{
  "name": "translation-validation",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "release:first": "npm run release -- --first-release",
    "deploy": "ng deploy --base-href=https://ngneat.github.io/libName/",
    "copy": "cp -r README.md dist/ngx-translation-validation",
    "build:lib": "ng build ngx-translation-validation --prod && npm run copy",
    "test:lib": "ng test ngx-translation-validation",
    "release": "cd projects/ngx-translation-validation && standard-version --infile ../../CHANGELOG.md",
    "test:lib:headless": "cross-env CI=true npm run test:lib",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.1.2",
    "@angular/common": "11.1.2",
    "@angular/compiler": "11.1.2",
    "@angular/core": "11.1.2",
    "@angular/forms": "11.1.2",
    "@angular/platform-browser": "11.1.2",
    "@angular/platform-browser-dynamic": "11.1.2",
    "@angular/router": "11.1.2",
    "@ngneat/lib": "1.0.6",
    "@ngneat/transloco": "2.20.0",
    "rxjs": "6.6.6",
    "tslib": "2.1.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1101.4",
    "@angular/cli": "11.1.4",
    "@angular/compiler-cli": "11.1.2",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/spectator": "6.1.3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jasmine": "3.6.4",
    "@types/node": "12.20.4",
    "all-contributors-cli": "6.20.0",
    "angular-cli-ghpages": "1.0.0-rc.1",
    "codelyzer": "6.0.1",
    "cross-env": "7.0.3",
    "git-cz": "4.7.6",
    "husky": "4.3.8",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.5.4",
    "ng-packagr": "11.2.4",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "semantic-release": "17.3.9",
    "standard-version": "9.1.1",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RiskChallenger/translation-validation.git"
  }
}
